<body>
<header>
   <nav class="navbar navbar-dark navbar-expand-md bg-dark">
      <div class="navbar-brand">
         <img src="https://www.google.com/images/branding/googleg/1x/googleg_standard_color_128dp.png" alt=""
            width="50%">
      </div>
      <button class="navbar-toggler" data-toggle="collapse" data-target="#conteudoNavbarSuportado">
         <span class="navbar-toggler-icon"></span>
      </button>
      <div class="collapse navbar-collapse" id="conteudoNavbarSuportado">
         <ul class="navbar-nav mr-auto">
            <li class="nav-item">
               <a href="/" class="nav-link">Home</a>
            </li>
            <li class="nav-item dropdown">
               <a href="#" class="nav-link dropdown-toggle" role="button" id="navbarDropdown" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                  Categorias
               </a>
               <div class="dropdown-menu" aria-labelledby="navbarDropdown">
                  <% categories.map(cat => { %>
                     <a href="/categories/<%= cat.slug %>" class="dropdown-item"><%= cat.title %></a>
                  <% }) %>
               </div>
            </li>
         </ul>
         <div class="float-right">
            <a href="/login" role="button" class="btn btn-primary">Login</a>
         </div>
      </div>
   </nav>
</header>